   "source": [
    "Char = input()\n",
    "s = ord(Char)\n",
    "s1 = s-1     #номер символу, що йде перед Char\n",
    "s2 = s+1     #номер символу, що йде після Char\n",
    "chr(s1), chr(s2)"
   ]
  }
